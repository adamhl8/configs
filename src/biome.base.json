        "noDuplicateProperties": "on",
        "noDuplicateSelectorsKeyframeBlock": "on",
        "noDuplicateTestHooks": "on",
        "noEmptyBlock": "on",
        "noEmptyBlockStatements": "on",
        "noEmptyInterface": "on",
